* {
    margin: 0;
    padding: 0;
}
#RootElement {
    flex-direction: column;
    align-items: flex-start;
    justify-content: flex-start;
    width: 100%;
    height: 100%;
}
#Icons {
    flex-direction: row;
    align-items: center;
    justify-content: space-evenly;
    width: 1152px;
    height: 200px;
    margin-left: 40px;
}
#SubIcon {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    width: 25%;
    height: 150px;
    align-self: flex-end;
}
#LifeText, #CoinText {
    font-size: 60px;
}
#LifeIcon, #CoinIcon{
    width: 150px;
    height: 100%;
}
#CoinIcon {
    background-image: url("project://database/Assets/Items/Designs/Coin Frame.png");
}
#BarShow {
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
    align-content: center;
    width: 1152px;
    height: 300px;
    margin-left: 40px;
}
#BarGroup {
    flex-direction: column;
    align-items: center;
    justify-content: space-evenly;
    align-content: center;
    height: 100%;
}
#Vitality {
    flex-direction: row;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 80px;
    align-self: flex-start;
}
#VitalityPiece {
    height: 100%;
    border-radius: 40px;
}
#RecoverVitality {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    border-width: 0px;
    width: 100%;
    height: 80px;
    align-self: center;
}
#RecoverVitalityPiece {
    height: 100%;
    border-radius: 20px;
}
#BunnyHop {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    border-width: 0px;
    width: 100%;
    height: 20px;
    align-self: flex-end;
}
#BunnyHopPiece {
    height: 100%;
    border-radius: 10px;
}
#FallDamageText {
    opacity: 0;
    font-size: 60px;
    margin-top: 20px;
    margin-left: 40px;
    align-self: flex-start;
}
