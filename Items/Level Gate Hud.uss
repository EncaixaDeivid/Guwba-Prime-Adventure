*{
	margin: 0px;
	padding: 0px;
}
#RootElement{
	width: 100%;
	height: 100%;
	flex-direction: row;
	align-items: center;
	justify-content: space-evenly;
	align-content: center;
}
#Level, #Boss, #Scenes{
	width: 25%;
	height: 39.0625%;
